{
  "manifest_version": 2,
  "name": "moodflow",
  "short_name": "moodflow",
  "description": "Replace the boring new tab page with beautiful scenery, while getting to know what makes you happy",
  "version": "0.0.0.10",
  "author": "Walter J. Monecke",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": { "scripts": ["hot-reload.js"] },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "https://www.youtube.com/iframe_api",
    "https://api.unsplash.com/photos/random"
  ]
}
